let arr=[
    {id:1,name:"shekhar",edit:false},
    {id:2,name:"vivek", edit:false},
];
const user={id:2,name:"sachin", edit:false};
arr=[...arr,user];
console.log(arr)
arr=[...arr,name="anjali"];
arr[1]={...arr,name:"abhi"}
console.log(arr)

